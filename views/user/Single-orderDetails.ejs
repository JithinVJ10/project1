	<div class="modal fade" id="exampleModal<%= order._id %>" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
		<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header">
			<h5 class="modal-title" id="exampleModalLabel">Comfirm</h5>
			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
				<span aria-hidden="true">&times;</span>
			</button>
			</div>
			<div class="modal-body">
			Are you Sure?
			</div>
			<div class="modal-footer">
			
			<% if (order.status === 'Delivered' && !order.returnExpired) { %>
				<a class="btn btn-sm btn-primary" href="/order-return/<%= order._id %>">Return Order</a>
			  <% } else if (order.status !== "Cancel" && order.status !== "Delivered" && order.status !== "Returned") { %>
				<a class="btn btn-sm btn-primary" href="/order-cancel/<%= order._id %>">Cancel Order</a>
			  <% } %>
			  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
			</div>
		</div>
		</div>
