			<div class="table-responsive">
				<table class="table text-start align-middle table-bordered table-hover mb-0">
					<p class="text-right text-muted">Return only available for 3 days after delivery*</p>
					<thead>
						
						<tr class="text-dark">
							
							<th scope="col">Date</th>
							<th scope="col">Order ID</th>
							<th scope="col">Product name</th>
							<th scope="col">Amount</th>
							<th scope="col">Address</th>
							<th scope="col">Image</th>
							<th scope="col">Payment Method</th>
							<th scope="col">Status</th>
							<th scope="col">Cancel/Return</th>

						</tr>
					</thead>
					<tbody>
						<tr> 
							<td><%= order.createdAt.toLocaleDateString() %></td>
							<td><%= order._id%></td>
							<td>
								<% order.items.forEach((item,index) =>{ %>

									<%= index+1 %>. <%= item.product.name %> <br>
								<% }) %>
							</td>
							<td>â‚¹<%= order.total %></td>
							<td>
								<%= order.address.name %><br>
								<%= order.address.mobile %><br>
								<%= order.address.address %><br>
								<%= order.address.city %><br>
							</td>
							<td>
								<% order.items.forEach((item,index) =>{ %>
									
									<img src="<%= item.product.photo[0] %>" alt="" width="40px" class="img-thumbnail"> <br>
								<% }) %>
							</td>
							<td><%= order.payment_method %></td>
							<td>
								<b><%= order.status %></b>
							</td>

							

								<td>
									<% if (order.status === 'Delivered' && !order.returnExpired) { %>
										<a class="btn btn-sm btn-primary" href="" data-toggle="modal" data-target="#exampleModal<%= order._id %>">Return</a>
									  <% } else if (order.status !== "Cancelled" && order.status !== "Delivered") { %>
										<a class="btn btn-sm btn-primary" href="" data-toggle="modal" data-target="#exampleModal<%= order._id %>">Cancel</a>
									  <% } else if(order.returnExpired){ %>
										<p>Return Not available</p>
									  <% } %>
								</td>
								
							
						</tr>
					   </a>
					  

					</tbody>
				</table>

				
